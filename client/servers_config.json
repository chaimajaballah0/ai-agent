{
    "mcpServers": {
      "search": {
        "command": "poetry run python",
        "args": ["./servers/website_browsing.py"],
        "env": {
                "SERPAPI_API_KEY": "${SERPAPI_API_KEY}"
            }
      },
      "website_browsing": {
        "command": "poetry run python",
        "args": ["./servers/website_browsing.py"]
      },
      "email": {
        "command": "poetry run python",
        "args": ["./servers/email.py"],
        "env": {
            "GMAIL_CREDS_FILE_PATH": "${GMAIL_CREDS_FILE_PATH}",
            "GMAIL_TOKEN_PATH": "${GMAIL_TOKEN_PATH}"
        }
      }
    }
  }